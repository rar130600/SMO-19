<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" styleClass="scene" stylesheets="@../smo.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AutoModeController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="20.0">
         <children>
            <TableView fx:id="sourceTable" prefHeight="300.0">
              <columns>
                <TableColumn fx:id="sourceTableColumnName" minWidth="100.0" prefWidth="-1.0" text="Имя" />
                  <TableColumn fx:id="sourceTableColumnCreated" minWidth="75.0" prefWidth="75.0" text="Создано" />
                <TableColumn fx:id="sourceTableColumnDone" minWidth="95.0" prefWidth="95.0" text="Выполнено" />
                  <TableColumn fx:id="sourceTableColumnReject" minWidth="90.0" prefWidth="90.0" text="Отклонено" />
                  <TableColumn fx:id="sourceTableColumnTimeInSystem" minWidth="160.0" prefWidth="-1.0" text="Ср.Время в системе" />
                  <TableColumn fx:id="sourceTableColumnProcTime" minWidth="170.0" prefWidth="-1.0" text="Ср.Время выполения" />
                  <TableColumn fx:id="sourceTableColumnWaitTime" minWidth="160.0" prefWidth="-1.0" text="Ср.Время ожидания" />
                  <TableColumn fx:id="sourceTableColumnWaitDisp" minWidth="130.0" prefWidth="-1.0" text="Дисп.Ожидания" />
                  <TableColumn fx:id="sourceTableColumnProcDisp" minWidth="145.0" prefWidth="-1.0" text="Дисп.Выполнения" />
                  <TableColumn fx:id="sourceTableColumnRejectProbability" minWidth="110.0" prefWidth="-1.0" text="Вер. Отказа" />
              </columns>
            </TableView>
            <TableView fx:id="deviceTable" prefHeight="300.0">
              <columns>
                <TableColumn fx:id="deviceTableColumnName" minWidth="100.0" prefWidth="100.0" text="Имя" />
                <TableColumn fx:id="deviceTableColumnK" minWidth="200.0" prefWidth="250.0" text="Коэффициент использования" />
              </columns>
            </TableView>
            <HBox alignment="CENTER_RIGHT" prefHeight="50.0" spacing="40.0">
               <children>
                  <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onClickExitButton" prefHeight="50.0" prefWidth="200.0" stylesheets="@../smo.css" text="Выход" textFill="WHITE">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="updateButton" mnemonicParsing="false" onAction="#onClickUpdateButton" prefHeight="50.0" prefWidth="200.0" stylesheets="@../smo.css" text="Обновить" textFill="WHITE">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
